<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mamorky.socialplayer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:hardwareAccelerated="false"
        android:largeHeap="true">
        <activity android:name=".ui.Song.SongViewImp" />
        <activity android:name=".ui.Album.AlbumViewImp"></activity>
        <activity android:name=".ui.Artist.ArtistViewImp" />
        <activity android:name=".ui.AboutActivity" />
        <activity android:name=".ui.PrincipalActivity">
        </activity>
        <activity android:name=".ui.Login.LoginViewImp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>